<!DOCTYPE html>

@using Microsoft.AspNetCore.Identity
@using EsecureModel.Usuario

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
@inject RoleManager<IdentityRole> RoleManager
<html>
<head>
    <meta charset="utf-8" />
    @*<meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta charset="utf-8">*@
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - eSafy</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/css/site.css" />
        @*<link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" crossorigin="anonymous" />*@
        <link rel="stylesheet" href="~/lib/bootswatch/spacelab/bootstrap.min.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="~/css/fontawesome-free-5.3.1-web/css/all.min.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
        <link rel="stylesheet" href="~/lib/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" />
        
        <link rel="stylesheet" href="~/lib/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" />
        <link rel="stylesheet" href="~/css/custom.css" />
        <link rel="stylesheet" href="~/css/custom-themes.css" />
    </environment>
    <environment include="Production">
        <link rel="stylesheet" href="~/css/site.css" />
        @*<link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" crossorigin="anonymous" />*@
        <link rel="stylesheet" href="~/lib/bootswatch/spacelab/bootstrap.min.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="~/css/fontawesome-free-5.3.1-web/css/all.min.css" crossorigin="anonymous" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
        <link rel="stylesheet" href="~/lib/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" />

        <link rel="stylesheet" href="~/lib/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" />
        <link rel="stylesheet" href="~/css/custom.css" />
        <link rel="stylesheet" href="~/css/custom-themes.css" />
    </environment>
</head>
<body class="bg-secondary">
    <link rel="shortcut icon" type="image/png" href="~/images/favicon.png" />
    <div class="container body-content card card-body">
        @RenderBody()
        <div class="form-group col-md-12">
            <!-- form de contacto -->
            <partial name="_VentanaSoporte" />
            <!-- form de mensajes -->
            <partial name="_ModalMsg2" />
        </div>
    </div>
    <footer class="footer fit-width bg-dark text-light">
        <div class="text-center">
            Versión 20190417.0200
        </div>
    </footer>
    <!-- page-wrapper -->
    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/popper.js/umd/popper-utils.min.js"></script>
        @*<script src="~/lib/twitter-bootstrap/js/bootstrap.min.js"></script>*@
        <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js"></script>
        <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
        <script type="text/javascript" src="~/lib/moment.js/moment.min.js"></script>
        <script type="text/javascript" src="~/lib/moment.js/moment-with-locales.min.js"></script>
        <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
        <script src="~/lib/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
        <script src="~/lib/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
        <script src="~/lib/Chart.js/Chart.min.js"></script>
        <script src="~/js/custom.js"></script>
        <script src="~/js/site.js"></script>
    </environment>
    <environment include="Production">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/popper.js/umd/popper-utils.min.js"></script>
        @*<script src="~/lib/twitter-bootstrap/js/bootstrap.min.js"></script>*@
        <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js"></script>
        <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
        <script type="text/javascript" src="~/lib/moment.js/moment.min.js"></script>

        <script type="text/javascript" src="~/lib/moment.js/moment-with-locales.min.js"></script>
        <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
        <script src="~/lib/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
        <script src="~/lib/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
        <script src="~/lib/Chart.js/Chart.min.js"></script>
        <script src="~/js/custom.js"></script>
        <script src="~/js/site.js"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
